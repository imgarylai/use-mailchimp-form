  "keywords": [
    "mailchimp",
    "react",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgarylai/use-mailchimp-form.git"
  },
  "author": "Gary Lai <garylai1990@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imgarylai/use-mailchimp-form/issues"
  },
